    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_app_dev_no_esp32",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define",
                "DEV=true",
                "--dart-define",
                "OFFLINE=true",
                "--dart-define",
                "IP=http://192.168.0.14:8000"
            ]
        },
        {
            "name": "flutter_app_dev",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define",
                "DEV=true",
                "--dart-define",
                "OFFLINE=false",
                "--dart-define",
                "IP=http://192.168.0.14:8000"
            ]
        },
        {
            "name": "flutter_app",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}